<h1>Listing rsakeys</h1>

<table>
  <tr>
    <th>Userid</th>
    <th>Marshkey</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @rsakeys.each do |rsakey| %>
  <tr>
    <td><%= rsakey.userid %></td>
    <td><%= Marshal.load(rsakey.marshkey) %></td>
    <td><%= link_to 'Show', rsakey %></td>
    <td><%= link_to 'Edit', edit_rsakey_path(rsakey) %></td>
    <td><%= link_to 'Destroy', rsakey, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

